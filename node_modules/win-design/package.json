  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "dist": "rm -rf ./dist && node build/build.js && npm run themes && npm run fonts",
    "version:update": "sh build/scripts/sync-version.sh",
    "fonts": "cp-cli packages/themes/fonts lib/themes/fonts",
    "themes": "sass packages/themes:lib/themes",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && npm run commit:log",
    "commit:log": "git add CHANGELOG.md && git commit -m 'docs: update changelog'"
  },
