  "description": "A UI Components Library with Vue2.x",
  "author": "wued",
  "main": "lib/win-design.js",
  "files": [
    "lib/*",
    "packages",
    "!packages/local.js"
  ],
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://172.16.0.115/winning-ued/win-design.git"
